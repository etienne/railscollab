- if @late_milestones.length > 0
  #lateMilestones.important
    .header= :late_milestones.l

    - if @late_milestones.length > 0
      %ul
        - @late_milestones.each do |milestone|
          %li
            = "#{:milestone_days_late.l_with_args(:days => milestone.days_late)}: #{:milestone_in.l_with_args(:milestone => link_to(h(milestone.name), milestone_path(:id => milestone.id, :active_project => milestone.project)), :project => link_to(h(milestone.project.name), project_path(:id => milestone.project.id)))}"
            = "(#{h milestone.assigned_to.object_name})" unless milestone.assigned_to.nil?

#myMilestones
  = milestones_calendar                                                                                             |
